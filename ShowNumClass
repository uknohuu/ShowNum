public class ShowNum{
    private static int num1;
    private static int num2;
    public ShowNum(int o){num1 = o;}
    public void ShowNum(int t){num2 = t;}
    public void setnum1(int o) { num1 = o;}
    public int getname() {return num1;}
    public void setnum2(int t) { num2 = t;}
    public int getName() {return num2;}
  
}
